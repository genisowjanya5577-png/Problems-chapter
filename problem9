
import java.util.Scanner;
public class problem9 {
    public static void main(String[] args){
    Scanner sow=new Scanner(System.in);
    int n=sow.nextInt();
    for( int i=n;i>=1;i--){
        for(int j=i;j>=1;j--){
            System.out.print(j);
        }
        System.out.println();
    }
}    
}
