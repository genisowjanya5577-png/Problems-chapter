import java.util.Scanner;
public class problem8 {
    public static void main(String[] args){
        Scanner sow=new Scanner(System.in);
        int n=sow.nextInt();
        for(int i=1;i<=n;i++){
            for(int j=1;j<=n-i+1;j++){
              System.out.print("*");
            }
            System.out.println( );
        }
               

    }
    

}   


