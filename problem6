
import java.util.Scanner;
public class problem6 {
    public static void main(String[] args){
        Scanner sow=new Scanner(System.in);
        System.out.println("Enter the value:");
        int x=sow.nextInt();
        for(int i=1;i*x<=1000;i++){
            System.out.println(x*i);
            
        }

    }
    
}
